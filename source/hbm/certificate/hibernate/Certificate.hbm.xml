<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="org.hisp.dhis.certificate.Certificate" table="certificate">
        <id name="id" type="int" column="id">
            <generator class="native"/>
        </id>
        <property name="Cid" column="cid" type="string"/>
        <property name="orgunit" column="orgunit" type="string"/>
        <property name="college" column="college" type="string"/>
        <property name="candidate" column="candidate" type="string"></property>
        <property name="dob" column="dob" type="date"></property>
        <property name="yearOfGraduation" column="yearofgraduation" type="date"></property>
        <property name="degreeClassification" column="degreeClassification" type="string"/>
        <property name="trainingSystem" column="trainingsystem" type="string"></property>
        <property name="major" column="major" type="string"/>

    </class>

</hibernate-mapping>